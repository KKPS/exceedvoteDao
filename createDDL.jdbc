CREATE TABLE USER (ID BIGINT AUTO_INCREMENT NOT NULL, FULLNAME VARCHAR(255), PASSWORD VARCHAR(255), USERNAME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE PROJECT (ID BIGINT AUTO_INCREMENT NOT NULL, DESCRIPTION VARCHAR(255), NAME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE QUESTION (ID BIGINT AUTO_INCREMENT NOT NULL, QUESTION VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE BALLOT (ID BIGINT AUTO_INCREMENT NOT NULL, user_id BIGINT, PROJECT_ID BIGINT, QUESTION_ID BIGINT, PRIMARY KEY (ID))
ALTER TABLE BALLOT ADD CONSTRAINT FK_BALLOT_PROJECT_ID FOREIGN KEY (PROJECT_ID) REFERENCES PROJECT (ID)
ALTER TABLE BALLOT ADD CONSTRAINT FK_BALLOT_QUESTION_ID FOREIGN KEY (QUESTION_ID) REFERENCES QUESTION (ID)
ALTER TABLE BALLOT ADD CONSTRAINT FK_BALLOT_user_id FOREIGN KEY (user_id) REFERENCES USER (ID)
